{
  "entrypoint": "index.html",
  "shell": "bank-web-app.html",
  "fragments": [
    "bank-web-app.html",
    "src\\bank-menu.html",
    "src\\bank-login-service.html",
    "src\\bank-login.html"
  ],
  "sources": [
    "src\\**\\*",
    "data\\**\\*",
    "images\\**\\*",
    "bower.json",
    "index.html",
    "bank-web-app.html",
    "bank-web-app.html",
    "src\\bank-menu.html",
    "src\\bank-login-service.html",
    "src\\bank-login.html"
  ],
  "extraDependencies": [
    "bower_components\\webcomponentsjs\\*.js",
    "bower_components\\webcomponentsjs\\*"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es5-bundled"
    },
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  }
}